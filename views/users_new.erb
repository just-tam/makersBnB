<body>

    <div class="limiter">
        <div class="container-login100" style="background-image: url('images/camel-bg.jpg');">
            <div class="wrap-login100">
                <form class="login100-form validate-form" action="/users" method="post">
                    <span class="login100-form-logo">
						<img src="images/nomads-logo.png">
					</span>
                    <br><br><br><br>
                    <span class="login100-form-title p-b-34 p-t-27">
						Register
					</span>
                    <div class="wrap-input100 validate-input" data-validate="Enter username">
                        <input class="input200" type="username" name="username" placeholder="Username ">
                        <span class="focus-input200 " data-placeholder="&#xf207; "></span>
                    </div>

                    <div class="wrap-input100 validate-input " data-validate="Enter email ">
                        <input class="input200" type="email" name="email " placeholder="Email">
                        <span class="focus-input200 " data-placeholder="&#xf109; "></span>
                    </div>

                    <div class="wrap-input100 validate-input " data-validate="Enter password ">
                        <input class="input200 " type="password" name="password" placeholder="Password ">
                        <span class="focus-input200 " data-placeholder="&#xf191; "></span>
                    </div>

                

                    <!-- <div class="contact100-form-checkbox ">
						<input class="input-checkbox100 " id="ckb1 " type="checkbox " name="remember-me ">
						<label class="label-checkbox100 " for="ckb1 ">
							Remember me
						</label>
					</div> -->

                    <div class="container-login100-form-btn">
                        <button class="login200-form-btn">
							Sign up!
                        </button>
                    </div>

                    <!-- <div class="text-center p-t-90 ">
                        <a class="txt1 " href="# ">
							Forgot Password?
						</a> -->
            </div>
            </form>
        </div>
    </div>
    </div>


    <div id="dropDownSelect1 "></div>

